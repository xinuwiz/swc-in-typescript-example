#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/xinuwiz/Documentos/react-example/swc-in-typescript-example/node_modules/.pnpm/@swc+cli@0.4.1-nightly.20240914_@swc+core@1.7.39/node_modules/@swc/cli/bin/node_modules:/home/xinuwiz/Documentos/react-example/swc-in-typescript-example/node_modules/.pnpm/@swc+cli@0.4.1-nightly.20240914_@swc+core@1.7.39/node_modules/@swc/cli/node_modules:/home/xinuwiz/Documentos/react-example/swc-in-typescript-example/node_modules/.pnpm/@swc+cli@0.4.1-nightly.20240914_@swc+core@1.7.39/node_modules/@swc/node_modules:/home/xinuwiz/Documentos/react-example/swc-in-typescript-example/node_modules/.pnpm/@swc+cli@0.4.1-nightly.20240914_@swc+core@1.7.39/node_modules:/home/xinuwiz/Documentos/react-example/swc-in-typescript-example/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/xinuwiz/Documentos/react-example/swc-in-typescript-example/node_modules/.pnpm/@swc+cli@0.4.1-nightly.20240914_@swc+core@1.7.39/node_modules/@swc/cli/bin/node_modules:/home/xinuwiz/Documentos/react-example/swc-in-typescript-example/node_modules/.pnpm/@swc+cli@0.4.1-nightly.20240914_@swc+core@1.7.39/node_modules/@swc/cli/node_modules:/home/xinuwiz/Documentos/react-example/swc-in-typescript-example/node_modules/.pnpm/@swc+cli@0.4.1-nightly.20240914_@swc+core@1.7.39/node_modules/@swc/node_modules:/home/xinuwiz/Documentos/react-example/swc-in-typescript-example/node_modules/.pnpm/@swc+cli@0.4.1-nightly.20240914_@swc+core@1.7.39/node_modules:/home/xinuwiz/Documentos/react-example/swc-in-typescript-example/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/swc.js" "$@"
else
  exec node  "$basedir/../../bin/swc.js" "$@"
fi
